          },
          "configurations": {
            "production": {}
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "web-next:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "web-next:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "web-next:build:production"
