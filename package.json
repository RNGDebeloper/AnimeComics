    "@types/sharp": "^0.31.0",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "regenerator-runtime": "^0.13.10",
    "sharp": "^0.31.2",
    "typescript": "^4.8.4"
