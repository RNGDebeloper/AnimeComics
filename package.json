    "lodash": "^4.17.21",
    "nanoid": "^3.3.3",
    "next": "^12.2.5",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "pdfkit": "^0.13.0",
    "react": "^18.2.0",
