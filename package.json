{
  "name": "rayriffy-h",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prepare": "husky install",
    "dev": "next",
    "build": "concurrently \"pnpm partytown\" \"pnpm prisma:generate && pnpm build:data\" && concurrently \"pnpm build:next\" \"pnpm sync:data\"",
    "build:next": "next build",
    "build:data": "node -r @swc-node/register tools/prebuildData.ts",
    "sync:data": "node -r @swc-node/register tools/sync.ts",
    "start": "next start",
    "partytown": "partytown copylib public/~partytown",
    "prisma:generate": "prisma generate",
    "prisma:commit": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rayriffy/rayriffy-h.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rayriffy/rayriffy-h/issues"
  },
  "homepage": "https://github.com/rayriffy/rayriffy-h#readme",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@builder.io/partytown": "^0.7.1",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@next/bundle-analyzer": "^13.0.2",
    "@prisma/client": "^4.5.0",
    "@storeon/crosstab": "^1.0.2",
    "@storeon/localstorage": "^1.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "axios": "^1.1.3",
    "copy-to-clipboard": "^3.3.2",
    "cwait": "^1.1.2",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21",
    "next": "^13.0.2",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "pdfkit": "^0.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storeon": "^3.1.5",
    "swr": "^1.3.0",
    "tailwindcss": "^3.2.1",
