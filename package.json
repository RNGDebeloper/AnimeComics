{
  "name": "rayriffy-h",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prepare": "husky install",
    "dev": "next",
    "build": "concurrently \"pnpm partytown\" \"pnpm prisma:generate && pnpm build:data\" && concurrently \"pnpm build:next\" \"pnpm sync:data\"",
    "build:next": "next build",
    "build:data": "node -r @swc-node/register tools/prebuildData.ts",
    "sync:data": "node -r @swc-node/register tools/sync.ts",
    "start": "next start",
    "partytown": "partytown copylib public/~partytown",
    "prisma:generate": "prisma generate",
    "prisma:commit": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rayriffy/rayriffy-h.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rayriffy/rayriffy-h/issues"
  },
  "homepage": "https://github.com/rayriffy/rayriffy-h#readme",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@builder.io/partytown": "^0.7.0",
