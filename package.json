{
  "name": "svelte",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm tem tools/patchSW.ts",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "fetch:data": "pnpm prisma:generate && pnpm tem tools/fetchData.ts",
    "sync:data": "pnpm tem tools/syncData.ts",
    "prisma:generate": "prisma generate",
    "prisma:commit": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "tem": "node --experimental-specifier-resolution=node --loader ts-node/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rayriffy/rayriffy-h.git"
  },
  "bugs": {
    "url": "https://github.com/rayriffy/rayriffy-h/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,svelte}": [
      "prettier --plugin-search-dir . --write",
      "git add"
    ]
  },
  "homepage": "https://h.rayriffy.com",
  "dependencies": {
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.12.0",
    "@storeon/crosstab": "^1.0.2",
    "@storeon/localstorage": "^1.5.0",
    "@storeon/svelte": "^1.0.0",
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/kit": "^1.15.0",
    "@types/cookie": "^0.5.1",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/sharp": "^0.31.1",
    "@vite-pwa/sveltekit": "^0.2.1",
    "autoprefixer": "^10.4.14",
    "daisyui": "^2.51.5",
    "dotenv": "^16.0.3",
    "lint-staged": "^13.2.0",
    "lodash.kebabcase": "^4.1.1",
    "p-queue": "^7.3.4",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "prisma": "^4.12.0",
    "storeon": "^3.1.5",
    "svelte": "^3.58.0",
    "svelte-aio": "^0.1.2",
    "svelte-check": "^3.1.4",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-pwa": "^0.14.7",
    "workbox-window": "^6.5.4"
  }
}
