{
  "name": "svelte",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "fetch:data": "pnpm prisma:generate && pnpm tem tools/fetchData.ts",
    "build:data": "node -r @swc-node/register tools/buildData.ts",
    "sync:data": "node -r @swc-node/register tools/syncData.ts",
    "prisma:generate": "prisma generate",
    "prisma:commit": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "tem": "node --experimental-specifier-resolution=node --loader ts-node/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rayriffy/rayriffy-h.git"
  },
  "bugs": {
    "url": "https://github.com/rayriffy/rayriffy-h/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,svelte}": [
      "prettier --plugin-search-dir . --write",
      "git add"
    ]
  },
  "homepage": "https://h.rayriffy.com",
  "dependencies": {
    "@hapi/accept": "^6.0.0",
    "@prisma/client": "^4.8.0",
    "@storeon/crosstab": "^1.0.2",
    "@storeon/localstorage": "^1.5.0",
    "@storeon/svelte": "^1.0.0",
    "axios": "^1.2.1",
    "daisyui": "^2.46.0",
    "lodash": "^4.17.21",
    "send": "^0.18.0",
    "sharp": "^0.31.3",
    "storeon": "^3.1.5",
    "svelte-aio": "^0.1.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@svicons/boxicons-regular": "^0.1.11",
    "@types/cookie": "^0.5.1",
    "@types/lodash": "^4.14.191",
    "@types/send": "^0.17.1",
    "@types/sharp": "^0.31.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "lint-staged": "^13.1.0",
    "p-queue": "^7.3.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prisma": "^4.8.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
